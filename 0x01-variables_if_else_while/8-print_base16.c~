#include <stdio.h>
#include <stdlib.h>
int main(void)
{
/* Variable Declearation */
int n;
char letter = 'a';
/* Intializing For Loop in the process*/
for (n = '0'; n <= '9'; n++)
{
putchar(n);
}
/* Intializing while loop */
while (letter <= 'f')
{
putchar(letter);
letter++;
}
putchar('\n');
return (0);
}
